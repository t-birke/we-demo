        <apex:includeScript value="{!URLFOR($Resource.MetronicResources, 'javascripts/respond.min.js')}"/>
        
        <script>
        var previousToolId;
        
        var previousToolUrl;
        
        var step1Visited = false;
        var step2Visited = false;
        var step3Visited = false;
        var step4Visited = false;
        
        //animations when the page loads.
        //$('.page-header').addClass('animated fadeInDown');
        //$('.page-sidebar-wrapper').addClass('animated fadeInLeft');
        
        $(document).ready(function() {
            Metronic.init();
            Layout.init();
            QuickSidebar.init();
            initializeSideBar();
            
            /* $('#authenticate').click(); */
            
            //Select the first tab of the wizard
            $('.tab-pane').first().addClass('active in');
            $('.nav-wizard').children().first().addClass('active');
            
            //Automatically expand the first of each accordion
            
            //$('.1accordionHeader').removeClass('collapsed');
            //$('.1accordionHeader').addClass('in');
            //$('.1accordionHeader').attr('aria-expanded','true');
            
            //$('.1accordionBody').removeClass('collapsed');
            //$('.1accordionBody').addClass('in');
            //$('.1accordionBody').attr('aria-expanded','true');
            
            //turn anchor tags from rich text fields into blue buttons
            $('.accordion').find('.panel-body').find('a').addClass('btn blue');
            $('.learnMore').removeClass('btn blue');
            
            $('.nav-header .arrow').addClass('open');
            
            
        });
        
        $('#sidebarSearch').keyup(function(){
            if($('#sidebarSearch').val() == '') {
                $('.glyphicon-search').fadeIn('fast');
                $('.glyphicon-remove').hide();
                $('.navTable').show();
                $('.tile').show();
            } else{
                $('.glyphicon-search').hide();
                $('.glyphicon-remove').fadeIn('fast');
                $('.navTable').hide();
                $('.tile').hide();
                $(".tile[data-search*='" + $('#sidebarSearch').val().toLowerCase() + "']").fadeIn('fast');
                $(".navTable[data-search*='" + $('#sidebarSearch').val().toLowerCase() + "']").fadeIn('fast');
            }
            
        });
        $('.glyphicon-remove').click(function(){
            $("#sidebarSearch").val('');
            $('.navLink').fadeIn('fast');
            $('.glyphicon-search').fadeIn('fast');
            $('.glyphicon-remove').hide();
        });
        
        $(".navLink").click( function() {
            console.log('nav')
            $('.navLink').parents('li').removeClass('active');
            $('.navLink').parent().next('td').removeClass('active');
            $(this).closest('li').addClass('active');
            $(this).parent().next('td').addClass('active');
            
            $(".videocontainer").hide();
            previousToolId = $(this).attr("id");
            setActive($(this).attr("id")); 
            setInstructions($(this).attr("id"));               
            previousToolUrl = $(this).attr("href");
            if($(this).attr("data-name").indexOf("Authenticate") > -1) {
                window.open( $(this).attr("href"),'popUpWindow','height=500,width=1000,left=100,top=100,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no, status=yes');
            } else {
                //if($('#SDOToolsHome').is(":visible")) 
                toggleActive('toolsnav');
                $('#lastTool').show();
                $('#SDOToolsHome').fadeOut();
                $('#newspage').fadeOut();
                $('#toolspage').fadeOut();
                $('#scriptsPage').fadeOut();
                $('#industriesPage').fadeOut();
                $(".toolIcons").fadeIn();
                $("#tool_frame").fadeIn();
                if($(this).data('new-window')){
                	window.open($(this).attr("href"));
                }
                else {
                    $("#tool_frame").attr("src", $(this).attr("href"));
                    $("#tool_frame").css("height", $(this).attr("data-name") == "Feedback" ? "550" : "2400");
                }
            }
            //return false;
            //showTool(this);
        });
        
        // right now this function is only called once, on click of the home icon to show the previous tool. id = null always
        function showToolById(id) {
            $(".videocontainer").hide();
            previousToolId = id;
            setActive($('#'+id).attr("id")); 
            setInstructions($('#'+id).attr("id"));               
            previousToolUrl = $('#'+id).attr("href");
            var attr = $('#'+id).attr("data-name");
            //if (typeof attr !== typeof undefined && attr !== false) {
            if(attr.indexOf("Authenticate") > -1) {
                window.open( $('#'+id).attr("href"),'popUpWindow','height=500,width=1000,left=100,top=100,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no, status=yes');
            } else {
                $('#lastTool').show();
                $('#SDOToolsHome').fadeOut();
                $('#newspage').fadeOut();
                $('#scriptsPage').fadeOut();
                $('#industriesPage').fadeOut();
                $('#toolspage').fadeOut();
                $(".toolIcons").fadeIn();
                $("#tool_frame").fadeIn();
                $("#tool_frame").attr("src", $('#'+id).attr("href"));
                $("#tool_frame").css("height", $('#'+id).attr("data-name") == "Feedback" ? "550" : "2400");                      
            }
        }
        
        function showTool(url) {
            if(url == null) url = previousToolUrl;
            $(".videocontainer").hide();
            previousToolUrl = url; 
            setInstructions();               
            $('#SDOToolsHome').fadeOut();
            $('#industriesPage').fadeOut();
            $('#scriptsPage').fadeOut();
            $('#toolspage').fadeOut();
            $(".toolIcons").fadeIn();
            $("#tool_frame").fadeIn();
            $("#tool_frame").attr("src", url);
            $("#tool_frame").css("height", $('#'+id).attr("data-name") == "Feedback" ? "550" : "2400");
            $('#newspage').fadeOut();                      
            //}
        }
        
        
        function initializeSideBar() {
            $('.navLink').each(function(i,v) {
                $(v).attr('data-search',$(v).attr('data-search').toLowerCase()); 
            });
            $('.navTable').each(function(i,v) {
                $(v).attr('data-search',$(v).attr('data-search').toLowerCase()); 
            });
            
        }
        
        function openAuthenticateWindow() {
            window.open('https://sdo-tools.herokuapp.com/auth/salesforce?returnURL=/auth/successful','popUpWindow','height=500,width=1000,left=100,top=100,resizable=yes,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no, status=yes');
        }
        
        function setActive(i) {
            $(".navLink").each( function() {
                if($(this).attr("id") == i) {
                    $(this).parent().addClass("active");
                } else {
                    $(this).parent().removeClass("active");
                }
            });
        }
        
        function setInstructions(id) {
            if($('#' + id + '-instructions').val() != '' && $('#' + id + '-instructions').val() != null) {
                $('#tool_info_box').show();
                // $('#tool_info_box').fadeIn();
                $('#tool_info_header').html($('#' + id + '-name').val());
                $('#tool_info_icon').attr('data-original-title',$('#' + id + '-i').val());
                $('#tool_info').html($('#' + id + '-instructions').val());
                id='\'' + id + '\'';
                $('#tool_info').append('<p><a href="#" data-toggle="modal" data-target="#learnMoreModal" class="learnMore" onclick="showMoreToolInformation('+id+');">Advanced Information</a></p>');
            }else {
                // $('#tool_info_box').fadeOut();
            }
            
        }
        
        function showMoreToolInformation(id) {
            console.log(id);
            var instructions = $('#' + id + '-advanced-instructions').val();
            console.log(instructions);
            var code = '{!JSENCODE('+ instructions+')}';
            showModal($('#' + id).attr('data-name') + ' Information',instructions);
        }
        
        function showModal(title, body){
            $('#myModalLabel').html(title);
            $('.modal-body').html(body);
            $('#learnMoreModal').focus();
        }
        
        function showHome() {
            if($('#tool_frame').is(":visible") || $('#toolspage').is(":visible")) {
                $('#tool_info_box').fadeOut();
                $('#tool_frame').fadeOut();
                //$('#toolspage').fadeOut();
                $(".toolIcons").fadeOut(); 
                $('#toolspage').hide();
            }
            $('#newspage').fadeOut();
            $('#industriesPage').fadeOut();
            $('#lastTool').hide();
            $('#scriptsPage').fadeOut();
            
            console.log(hideWelcome);
            $('#SDOToolsHome').fadeIn();
            if (hideWelcome) {
                $('.myWalkthrough').fadeOut();
                /*
                  $('#toolspage').show();
                  $('#toolspage').fadeIn();
                  */
                }
                resetWelcome();
            }
        function showNews(){
            if($('#tool_frame').is(":visible") || $('#toolspage').is(":visible")) {
                $('#tool_info_box').fadeOut();
                $('#tool_frame').fadeOut();
                //$('#toolspage').fadeOut();
                $(".toolIcons").fadeOut(); 
            }
            $('#toolspage').fadeOut();
            $('#industriesPage').fadeOut();
            $('#SDOToolsHome').fadeOut();
            //if(previousToolUrl != null) $('#lastTool').show();
            $('#scriptsPage').fadeOut();
            $('#newspage').show();
            $('#newspage').fadeIn();
            
        }
        
        function showTools() {
            if($('#tool_frame').is(":visible") || $('#SDOToolsHome').is(":visible")) {
                $('#tool_info_box').fadeOut();
                $('#tool_frame').fadeOut();
                $('#SDOToolsHome').fadeOut();
                $(".toolIcons").fadeOut(); 
            }
            $('#scriptsPage').fadeOut();
            $('#newspage').fadeOut();
            $('#scriptsPage').fadeOut();
            $('#industriesPage').fadeOut();
            //if(previousToolUrl != null) $('#lastTool').show();
            $('#toolspage').show();
            $('#toolspage').fadeIn();
        }
        function showScripts() {
            $('#tool_info_box').fadeOut();
            $('#tool_frame').fadeOut();
            $('#SDOToolsHome').fadeOut();
            $(".toolIcons").fadeOut(); 
            $('#newspage').fadeOut();
            $('#toolspage').fadeOut();
            $('#industriesPage').fadeOut();
            
            $('#scriptsPage').fadeIn();
        }
        function showIndustries() {
            $('#tool_info_box').fadeOut();
            $('#tool_frame').fadeOut();
            $('#SDOToolsHome').fadeOut();
            $(".toolIcons").fadeOut(); 
            $('#newspage').fadeOut();
            $('#toolspage').fadeOut();
            
            $('#scriptsPage').fadeOut();
            $('#industriesPage').fadeIn();
        }
        
        
        function showPage(id,id2){
            $('#tool_info_box').fadeOut();
            $('#tool_frame').fadeOut();
            $('#SDOToolsHome').fadeIn();
            $(".toolIcons").fadeOut(); 
            $('#scriptsPage').fadeOut();
            $('#newspage').fadeOut();
            $('#toolspage').fadeOut();
            $('#industriesPage').fadeOut();
            
            $('#lastTool').hide();
            if(id == 'StartHere' && previousToolUrl != null) $('#lastTool').show();
            
            $('#'+id).fadeIn();
            if(id2) $('#'+id2).fadeIn();
        }
        
        function toggleActive(c) {
            
            $('.homenavtab').removeClass('active');
            $('.homenavtab').children('.selected').remove();
            $('.'+c).addClass('active');
            $('.'+c).prepend( "<span class='selected'></span>" );
        }
        
        var hideWelcome = false;
        function welcomeButton(pageName){
            
            //$('.welcomeWrapper').hide();
            if(pageName == 'tools'){
                showTools();
                toggleActive('toolsnav');
            }else if(pageName == 'news'){
                toggleActive('newsnav');
                showNews();
            }else {
                hideWelcome = true;
                animateWelcome();
                $('#SDOToolsHome').fadeIn();
                $('.myWalkthrough').fadeIn();
                //showHome();
            }
        }
        
        function animateWelcome() {
            $('.welcomeWrapper').css('padding-top','30px');
            $('.welcomeWrapper').css('min-height','80px'); 
            
            $('.welcomeImgWrapper').css('width','125px')
            
            $('.bigText').css('position','relative')
            $('.bigText').css('top','0px')
            
            $('.welcomeImg').css('height','100px');
            $('.welcomeImg').css('width','100px');
            
            $('.welcomeText').fadeOut('fast');
            $('.welcomeBtn').fadeOut('fast');
            $('.fixedwidthwrapper').css('margin','0');
        }
        
        function resetWelcome() {
            $('.welcomeWrapper').css('padding-top','30px');
            $('.welcomeWrapper').css('min-height','735px'); 
            
            $('.welcomeImgWrapper').css('width','33%')
            
            $('.bigText').css('position','relative')
            $('.bigText').css('top','0px')
            
            $('.welcomeImg').css('height','100%');
            $('.welcomeImg').css('width','100%');
            
            $('.welcomeText').fadeIn('slow');
            $('.welcomeBtn').fadeIn('slow');
            $('.fixedwidthwrapper').css('margin','0px auto');
        }
        function selectChevron(id) {
            showPage('StartHere','SDOToolsHome');
            welcomeButton('walkthrough');
            toggleActive('startHereNav');
            
            $('.cloudLi').removeClass('active');
            $('#'+id).addClass('active');
        }
        function trackClick(clickName) {
            _gaq.push(['_trackEvent','Lead','Reset',clickName]);
            return false;
        }
        
        
        
        
        //Google Analytics Code
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
                                })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
        
        ga('create', 'UA-46606102-1', 'salesforce.com');
        ga('send', 'pageview');
        
        
        
        </script>
    </body>
</apex:page>