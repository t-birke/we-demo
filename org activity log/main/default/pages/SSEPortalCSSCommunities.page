            box-shadow: inset 0px 0px 6px -1px #333 !important;
            -webkit-box-shadow: inset 0px 0px 6px -1px #333 !important;
            <apex:outputPanel rendered="{!userTemplate.Image_Button__c == true}" layout="none">
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}/resource/SSEPortalResources/images/SSEPortalBarBackground.png') {!userTemplate.Back_Button_G_T__c} !important;
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -moz-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%) !important; 
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_G_B__c})) !important;
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%) !important;
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), linear-gradient(to bottom,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%) !important;
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Image_Button__c == false}" layout="none">
                background: {!userTemplate.Back_Button_G_T__c} !important;
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%) !important; 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_G_B__c})) !important;
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%) !important;
                background: linear-gradient(to bottom,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%) !important;
            </apex:outputPanel>
        }
        .btn:hover, body .menuButton .menuButtonButton:hover, .chatterFileListBlock .multiUploadControl .dragDropZoneDiv .dragDropZoneButton:hover{
            color:{!userTemplate.Font_Colour_Button_H__c} !important;
            font-size:{!userTemplate.Font_Size_Button_H__c} !important;
            font-weight:{!userTemplate.Font_Weight_Button_H__c} !important;
            text-transform:{!userTemplate.Font_Transform_Button_H__c} !important;
            text-decoration:{!userTemplate.Font_Decoration_Button_H__c} !important;
            border:0px;
            background:none !important;
            <apex:outputPanel rendered="{!userTemplate.Image_Button_H__c == true}" layout="none">
