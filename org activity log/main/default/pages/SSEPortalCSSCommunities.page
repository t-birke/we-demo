<apex:page cache="false" showHeader="false" contentType="text/css" controller="SSEPortalCSSController">
        
    /*CSS for communities. Change at your own risk.
    Portal CSS additions should be put in SSEPortalCSSEdit*/
    
    /*Page positioning and size*/
        #portalbody {
            padding:0px;
            <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
                background: {!userTemplate.Back_Body_G_T__c};
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Body_G_T__c} 0%, {!userTemplate.Back_Body_G_B__c} 100%); 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Body_G_T__c}), color-stop(100%,{!userTemplate.Back_Body_G_B__c}));
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Body_G_T__c} 0%,{!userTemplate.Back_Body_G_B__c} 100%);
                background: linear-gradient(to bottom,  {!userTemplate.Back_Body_G_T__c} 0%,{!userTemplate.Back_Body_G_B__c} 100%);
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Position_Page__c != 'classic'}" layout="none">
                margin-top:5px;
            </apex:outputPanel>
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            width:993px;
            display:table;
            min-height:900px;
        }
        table.outerNoSidebar{
            left:0px;
            width: 993px;
        }
        #portalwrapper{
            width:100%;
            <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
                width:993px;
            </apex:outputPanel>
            min-height:100px;
            display:table;
            <apex:outputPanel rendered="{!userTemplate.Shadow_Body__c == true}" layout="none">
                box-shadow:0px 0px 10px #4d4d4d;
                -webkit-box-shadow:0px 0px 10px #4d4d4d;
                -moz-box-shadow:0px 0px 10px #4d4d4d;
            </apex:outputPanel>        
            margin:auto;
        }
        #portalfooter{
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
        }
        #portalheader{
            height:365px;
            position: relative;
            z-index: -1;
            <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
                box-shadow:none;
                -webkit-box-shadow:none;
                -moz-box-shadow:none;
            </apex:outputPanel>
        }
        #portalheader, #portalfooter, .bodyDiv{
            width:100% !important;
            margin:auto;
        }
        #SSEPortalLogo{
            width:993px !important;
        }
        #portalheadertop{
            position:absolute;
            z-index:99;
        }
        #bodyCell{
            width:973px;
            padding-left:10px;
            padding-right:10px;
            left:0px;
        }
        #sidebarCell, #sidebarDiv .sidebarModule, #sidebarDiv .nestedModule{
            display:none
        }
        body{
            min-width:0px;
        }
        .zen .zen-main{
            overflow:visible;
            margin-top: 10px;
        }
        .zen #search{
            background: #fff;
        }
        body.hasMotif{
            padding: 0px;
        }
        .threecolumn .headerContent .helpLink, .threecolumn .headerContent .helpIcon{
            display:none;
        }
        
    /*Hide Slider*/
        #portalheader{
            height:100px;
        }
        #portalheaderbot{
            display:none;
        }
        #tabBar{
            position: relative;
            top:270px;
        }
    
    /*Global Header Bar*/
        .globalHeaderBar{
            width:993px;
            margin:auto;
            z-index: 100;
            position: relative;
            background:none;
        }
        .globalHeaderCommunityMenuContainer .globalHeaderVerticalLineInner{
            border-right:1px solid {!userTemplate.Back_Header_T_G_T__c};
        }
        .globalHeaderCommunityMenuContainer, .globalHeaderCommunityMenuContainer .globalHeaderVerticalLineOuter{
            border-right:1px solid {!userTemplate.Back_Navigation_Menu__c};
        }
        .globalHeaderNameMenuContainer .globalHeaderVerticalLineInner{
            border-left:1px solid {!userTemplate.Back_Header_T_G_T__c};
        }
        .globalHeaderNameMenuContainer, .globalHeaderNameMenuContainer .globalHeaderVerticalLineOuter{
            border-left:1px solid {!userTemplate.Back_Navigation_Menu__c};
        }
        .globalHeaderBar .zen-options{
            border-top:1px solid {!userTemplate.Back_Header_T_G_B__c};
        }
        .globalHeaderBar .zen-open{
            background:{!userTemplate.Back_Navigation_Menu__c};
        }
        .globalHeaderBar .zen-options{
            background:{!userTemplate.Back_Navigation_Menu__c};    
        }
        .globalHeaderBar .zen-options li, .globalHeaderBar .zen-open .zen-trigger{
            border-bottom:1px solid {!userTemplate.Back_Navigation_Menu_H__c};
        }
        .globalHeaderBar .zen-options .zen-firstItem{
            border-top:1px solid {!userTemplate.Back_Navigation_Menu_H__c};
        }
        .globalHeaderBar .zen-options a,.globalHeaderBar .zen-select a{
            color:{!userTemplate.Font_Colour_Navigation_Menu__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu__c};   
        }
        .globalHeaderBar .zen-open .zen-options a:hover, .globalHeaderBar .zen-open .zen-options a:focus{
            background:{!userTemplate.Back_Navigation_Menu_H__c};
            cursor:pointer;
            color:{!userTemplate.Font_Colour_Navigation_Menu_H__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu_H__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu_H__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu_H__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu_H__c};   
        }
        .globalHeaderBar .zen-select a:hover, .globalHeaderBar .zen-select a:focus{
            color:{!userTemplate.Font_Colour_Navigation_Menu_H__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu_H__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu_H__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu_H__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu_H__c};  
        }
        
    /*Chatter Communities Colours*/
        .zen .zen-page{
            padding:0px;
            background:transparent;
        }
        .zen .zen-bodyZen, .bodyDiv, .zen .zen-headerBottom {
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            background:none;
            border: 0px;
        }
        .FileDetailPage .filePreviewHeading{
            display:none;
        }
        html .brandQuaternaryBgr {
            <apex:outputPanel rendered="{!userTemplate.Image_Background__c == true}" layout="none">
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}') {!userTemplate.Back_Background_G_T__c} !important;   
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -moz-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%, {!userTemplate.Back_Background_G_B__c} 100%) !important; 
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Background_G_T__c}), color-stop(100%,{!userTemplate.Back_Background_G_B__c})) !important;
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -webkit-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%) !important;
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), linear-gradient(to bottom,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%) !important;
                background-repeat:{!userTemplate.Image_Background_Repeat__c};
                background-position:{!userTemplate.Image_Background_Position__c};
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Image_Background__c == false}" layout="none">
                background: {!userTemplate.Back_Background_G_T__c} !important;   
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%, {!userTemplate.Back_Background_G_B__c} 100%) !important; 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Background_G_T__c}), color-stop(100%,{!userTemplate.Back_Background_G_B__c})) !important;
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%) !important;
                background: linear-gradient(to bottom,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%) !important;
            </apex:outputPanel>
            margin:0px;
        }
        
   
        .bodyDiv{
            top:0px;
            min-height:600px;
        }
        .publishercontainer.defaultState #publishersharebutton, .zen .zen-highlightBtn{
            background: none;
            background: {!userTemplate.Back_Side_Button_G_T__c};
            background: -moz-linear-gradient(top, {!userTemplate.Back_Side_Button_G_T__c}  0%, {!userTemplate.Back_Side_Button_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Side_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Side_Button_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Side_Button_G_T__c}  0%,{!userTemplate.Back_Side_Button_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Side_Button_G_T__c}  0%,{!userTemplate.Back_Side_Button_G_B__c} 100%);
            border-radius:{!userTemplate.Radius_Side_Button__c};
            -webkit-border-radius:{!userTemplate.Radius_Side_Button__c};
            -moz-border-radius:{!userTemplate.Radius_Side_Button__c};
            box-shadow:none;
            -webkit-box-shadow:none;
            border:0px;
        }
        .publishercontainer.defaultState #publishersharebutton:hover, .zen .zen-highlightBtn:hover, .zen .zen-btn.zen-highlightBtn:hover, .zen .zen-btn.zen-highlightBtn.onHover{
            background: none;
            background: {!userTemplate.Back_Side_Button_H_G_T__c};
            background: -moz-linear-gradient(top, {!userTemplate.Back_Side_Button_H_G_T__c}  0%, {!userTemplate.Back_Side_Button_H_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Side_Button_H_G_T__c} ), color-stop(100%,{!userTemplate.Back_Side_Button_H_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Side_Button_H_G_T__c}  0%,{!userTemplate.Back_Side_Button_H_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Side_Button_H_G_T__c}  0%,{!userTemplate.Back_Side_Button_H_G_B__c} 100%);
            border-radius:{!userTemplate.Radius_Side_Button__c};
            -webkit-border-radius:{!userTemplate.Radius_Side_Button__c};
            -moz-border-radius:{!userTemplate.Radius_Side_Button__c};
            box-shadow:none;
            -webkit-box-shadow:none;
            border:0px;
            cursor:pointer;
        }
        .entityFeedLayout{
            background:none;
        }


        <apex:outputPanel rendered="{!userTemplate.Position_Slider__c == 'hide'}" layout="none">
            #portalheader{
                height:100px !important;
            }
            #portalheadermid{
                top:0px !important;
            }
            #portalheaderbot{
                display:none !important;
            }
            #tabBar{
                top:0px !important;
            }
        </apex:outputPanel>
        
        .slides_container, .slides_container div, .slides_control{
            width:993px !important;
        }
        .slides_container div{
            background-size: 100% 100%;
        }
        <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
            #portalfooter{
                background-size: 100% 100%;
            }
        </apex:outputPanel>
    /*Nav bar*/
        html .brandPrimaryBgr {
            background:none !important;
        }
        .zen #tabContainer{
            background:none !important;
        }
        #tabBar{
            padding:0px;
            height: 40px;
            line-height: 40px;
        }
        .zen-firstItem{
            margin-left:0px !important;
        }
        .zen .zen-tabMenu{
            margin:0px !important;
            border-radius:{!userTemplate.Radius_Tab__c} !important;
            -webkit-border-radius:{!userTemplate.Radius_Tab__c} !important;
            -moz-border-radius:{!userTemplate.Radius_Tab__c} !important;
            <apex:outputPanel rendered="{!userTemplate.Position_Tabs__c == 'classic'}" layout="none">
                background: {!userTemplate.Back_Tab_G_T__c} !important;
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Tab_G_T__c}  0%, {!userTemplate.Back_Tab_A_G_B__c} 100%) !important; 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Tab_G_T__c} ), color-stop(100%,{!userTemplate.Back_Tab_G_B__c})) !important;
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Tab_G_T__c} 0%,{!userTemplate.Back_Tab_G_B__c} 100%) !important;
                background: linear-gradient(to bottom,  {!userTemplate.Back_Tab_G_T__c}  0%,{!userTemplate.Back_Tab_G_B__c} 100%) !important; 
            </apex:outputPanel>
        }
        .zen .zen-tabMenu .zen-active{
            border-radius:{!userTemplate.Radius_Tab__c} !important;
            -webkit-border-radius:{!userTemplate.Radius_Tab__c} !important;
            -moz-border-radius:{!userTemplate.Radius_Tab__c} !important;
            background: none;
            background: {!userTemplate.Back_Tab_A_G_T__c} !important;
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Tab_A_G_T__c}  0%, {!userTemplate.Back_Tab_A_G_B__c} 100%) !important; 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Tab_A_G_T__c} ), color-stop(100%,{!userTemplate.Back_Tab_A_G_B__c})) !important;
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Tab_A_G_T__c} 0%,{!userTemplate.Back_Tab_A_G_B__c} 100%) !important;
            background: linear-gradient(to bottom,  {!userTemplate.Back_Tab_A_G_T__c}  0%,{!userTemplate.Back_Tab_A_G_B__c} 100%) !important;
            border:0px;
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            margin:0px;
            height: 40px;
            line-height: 40px;
        }
        .zen .zen-tabMenu li{
            border-radius:{!userTemplate.Radius_Tab__c} !important;
            -webkit-border-radius:{!userTemplate.Radius_Tab__c} !important;
            -moz-border-radius:{!userTemplate.Radius_Tab__c} !important;
            background: none;
            border:0px;
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            margin:0px;
            height: 40px;
            line-height: 40px;
        }
        .zen .zen-tabMenu li:hover, .zen .zen-tabMenu .zen-active:hover{
            background: none;
            background: {!userTemplate.Back_Tab_H_G_T__c} !important;
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Tab_H_G_T__c}  0%, {!userTemplate.Back_Tab_H_G_B__c} 100%) !important; 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Tab_H_G_T__c} ), color-stop(100%,{!userTemplate.Back_Tab_H_G_B__c})) !important;
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Tab_H_G_T__c} 0%,{!userTemplate.Back_Tab_H_G_B__c} 100%) !important;
            background: linear-gradient(to bottom,  {!userTemplate.Back_Tab_H_G_T__c}  0%,{!userTemplate.Back_Tab_H_G_B__c} 100%) !important;
            border:0px;
            box-shadow:none;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            cursor:pointer;
        }
        .zen .zen-tabMenu .zen-active a{
            color:{!userTemplate.Font_Colour_Tab_A__c} !important;
            font-size:{!userTemplate.Font_Size_Tab_A__c} !important;
            font-weight:{!userTemplate.Font_Weight_Tab_A__c} !important;
            text-transform:{!userTemplate.Font_Transform_Tab_A__c} !important;
            text-decoration:{!userTemplate.Font_Decoration_Tab_A__c} !important;
        }
        .zen .zen-tabMenu li a{
            color:{!userTemplate.Font_Colour_Tab__c} !important;
            font-size:{!userTemplate.Font_Size_Tab__c} !important;
            font-weight:{!userTemplate.Font_Weight_Tab__c} !important;
            text-transform:{!userTemplate.Font_Transform_Tab__c} !important;
            text-decoration:{!userTemplate.Font_Decoration_Tab__c} !important;    
        }
        .zen .zen-tabMenu li a:hover, .zen .zen-tabMenu .zen-active a:hover, .zen .zen-tabMenu > li:hover a, .zen .zen-tabMenu li:hover a, .zen .zen-tabMenu .zen-active:hover a{
            color:{!userTemplate.Font_Colour_Tab_H__c} !important;
            font-size:{!userTemplate.Font_Size_Tab_H__c} !important;
            font-weight:{!userTemplate.Font_Weight_Tab_H__c} !important;
            text-transform:{!userTemplate.Font_Transform_Tab_H__c} !important;
            text-decoration:{!userTemplate.Font_Decoration_Tab_H__c} !important;
        }
        /*Nav bar and header at top or bottom*/
        <apex:outputPanel rendered="{!userTemplate.Position_Tabs__c != 'classic'}" layout="none">
            .zen .zen-bodyZen, .bodyDiv{
                top:-29px;
            }
            nav {
                float: right;
                position: relative;
                top: -350px;
                <apex:outputPanel rendered="{!userTemplate.Position_Slider__c == 'hide'}" layout="none">
                    top: -75px !important;
                </apex:outputPanel>
            }
            .zen .zen-tabMenu li{
                background: {!userTemplate.Back_Tab_G_T__c} !important;
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Tab_G_T__c}  0%, {!userTemplate.Back_Tab_G_B__c} 100%) !important; 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Tab_G_T__c} ), color-stop(100%,{!userTemplate.Back_Tab_G_B__c})) !important;
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Tab_G_T__c} 0%,{!userTemplate.Back_Tab_G_B__c} 100%) !important;
                background: linear-gradient(to bottom,  {!userTemplate.Back_Tab_G_T__c}  0%,{!userTemplate.Back_Tab_G_B__c} 100%) !important;
                margin-left:10px;
            }
            .zen .zen-tabMenu .zen-active{
                margin-left:10px;
            }
            <apex:outputPanel rendered="{!userTemplate.Position_Tabs__c == 'bottom'}" layout="none">
                nav{
                    top:-65px;
                }
            </apex:outputPanel>
        </apex:outputPanel>
        
    /*Buttons*/
    /*Bug in communities which stops the next style from being picked up. Neat.*/
        .btn{
    
        }
        .btn, body .menuButton .menuButtonButton, .chatterFileListBlock .multiUploadControl .dragDropZoneDiv .dragDropZoneButton{
            color:{!userTemplate.Font_Colour_Button__c} !important;
            font-size:{!userTemplate.Font_Size_Button__c} !important;
            font-weight:{!userTemplate.Font_Weight_Button__c} !important;
            text-transform:{!userTemplate.Font_Transform_Button__c} !important;
            text-decoration:{!userTemplate.Font_Decoration_Button__c} !important;
            background:none !important;
            padding: 3px !important;
            padding-right: 5px !important;
            border: 0px !important;
            padding-left: 5px !important;
            border-radius: {!userTemplate.Radius_Button__c} !important;
