<!-- Based on from https://sam-cousins.com/embedding-twitter-feeds-on-lightning-pages/ -->
<apex:page >
    <div align="center">
        <blockquote class="instagram-media" data-instgrm-version="7" style="background:#FFF; border:0; border-radius:3px; box-shadow:0 0 1px 0 rgba(0,0,0,0.5),0 1px 10px 0 rgba(0,0,0,0.15); margin: 1px; max-width:320px; min-width:500px; padding:0; width:99.375%; width:-webkit-calc(100% - 2px); width:calc(100% - 2px);">
            <a href="https://www.instagram.com/p/{!$CurrentPage.Parameters.PictureId}/"></a> 
        </blockquote>
        <script src="//platform.instagram.com/en_US/embeds.js"></script>
    </div>
</apex:page>