<apex:page showheader="false">
   
    <apex:composition template="SDO_Bootstrap_Template"/>


<head>
    <title>Contact Us Web2Lead</title>

    <META HTTP-EQUIV="Content-type" CONTENT="text/html; charset=UTF-8">
    <style type="text/css">
    
        table {
            margin-left:20px;
            font: 12px Verdana, Arial, sans-serif;
        }
        
        table caption {
            text-align:left;
        }
        
        body {
            background-color:#fff;
            border:0;margin:0;
        }
        
        h1 {
            font: 22px verdana, arial, sans-serif normal;
            margin-bottom:5px;
        }
        
        
        
        form input {
            font: 12px Verdana, Arial, sans-serif;
            width:225px;
            height:16px;
            _height:20px;
            color: #000;
            border:1px solid #A7A6AA;
            float:left;
            padding:3px 1px 2px 1px;
            _padding:0;
            margin:0;
        }
        
        form label {
            font: 12px Verdana, Arial, sans-serif;
        }
        
        form select {
            font: 12px Verdana, Arial, sans-serif;
            width:225px;
            height:20px;
            _height:24px;
            color: #000;
            border:1px solid #A7A6AA;
            float:right;
            padding:2px 1px 3px 0px;
            margin:0 4px 0 0;
                
        }
        
        .multiselectpl {
            height:75px;
        }
        
        form #submit {
            font: 12px Verdana, Arial, sans-serif;
            padding-bottom:5px;
            padding-left:0;
            padding-right:0;
            padding-top:1px;
            height:20px;
            width:75px;
            float:left;
        }
        
        form textarea {
            font: 12px Verdana, Arial, sans-serif;
            width:200px;
            color: #000;
            padding:3px 1px 2px 1px;
            border:1px solid #A7A6AA;
            height:50px;
        }
    </style>

</head>
<script type="text/javascript">
        function initFormURL(){
        document.getElementById("thanksURL").value = "https://" + window.location.host + "/apex/W2LW2C_Landing_Page";
        }
    </script>
<body onload="initFormURL();">
    <table border="0" align="center" style="margin-left:auto;margin-right:auto;">
        <tbody>
            <tr>
              <td colspan="3"><img src="{!$Resource.UCIBanner}"></td>
            </tr>
            <tr>
                <td align="left" valign="top">
                
                    <!-- if the company's web site has content in its left column, paste that content here -->
                    <!-- make sure the left column content ends here -->
                
                </td>
                <td valign="top">
                    
                    <!-- if the company's web site has content in its middle column above the Web2Lead form, paste that content here -->
                    
                    <h1>Contact {!$Organization.Name}</h1>
        
                    Please complete this form and one of our representatives will contact you shortly.
                    <br/><br/><br/>
                    <sup style="color:#ff0000;">*</sup> indicates a required field.</span>
                    <br/><br/>
                    
                    <!-- make sure the middle column content ends here -->
        
                    <!-- BEGIN WEB-2-LEAD FORM -->
                    
                    <form action="https://www.salesforce.com/servlet/servlet.WebToLead?encoding=UTF-8" method="POST">
                
                        <input type=hidden name="oid" value="{!$Organization.Id}">
                        <input type=hidden name="retURL" id="thanksURL">
                
                        <input type=hidden name="lead_source" id="lead_source" value="Web Direct">
                        <input type=hidden name="Campaign_ID" value="7011t00000017oW">
                        <input type=hidden name="member_status" value="responded">
                
                        <table>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="first_name"><sup style="color:#ff0000;">*</sup>First Name</label>
                                </td>
                                <td>
                                    <input  id="first_name" maxlength="40" name="first_name" size="20" type="text" value="Tom" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="last_name"><sup style="color:#ff0000;">*</sup>Last Name</label>
                                </td>
                                <td>
                                    <input  id="last_name" maxlength="80" name="last_name" size="20" type="text"  value="Samsonite" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="title">Title</label>
                                </td>
                                <td>
                                    <input  id="title" maxlength="40" name="title" size="20" type="text" value="CEO" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="company"><sup style="color:#ff0000;">*</sup>Company</label>
                                </td>
                                <td>
                                    <input  id="company" maxlength="40" name="company" size="20" type="text" value="United Partners*" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="street"><sup style="color:#ff0000;">*</sup>Address</label>
                                </td>
                                <td>
                                    <textarea name="street">303 Bay Street</textarea>
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="city"><sup style="color:#ff0000;">*</sup>City</label>
                                </td>
                                <td>
                                    <input  id="city" maxlength="40" name="city" size="20" type="text" value="San Francisco" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="state"><sup style="color:#ff0000;">*</sup>State/Province</label>
                                </td>
                                <td>
                                    <input  id="state" maxlength="20" name="state" size="20" type="text" value="CA" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="zip"><sup style="color:#ff0000;">*</sup>Zip</label>
                                </td>
                                <td>
                                    <input  id="zip" maxlength="20" name="zip" size="20" type="text" value="94133" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="phone">Phone</label>
                                </td>
                                <td>
                                    <input id="phone" maxlength="40" name="phone" size="20" type="text" value="(415) 901-7000" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="email"><sup style="color:#ff0000;">*</sup>Email</label>
                                </td>
                                <td>
                                    <input  id="email" maxlength="80" name="email" size="20" type="text" value="{!$User.Email}" />
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="00N1t0000027XoO">What is your product interest?</label>
                                </td>
                                <td>
                                    <select  id="00N1t0000027XoO" name="00N1t0000027XoO" title="Product Interest">
                                        <option value="">--None--</option>
                                        <option value="Products">Products</option>
                                        <option value="Services">Services</option>
                                        <option value="Support">Support</option>
                                        <option value="Consulting">Consulting</option>
                                        <option value="Compliance">Compliance</option>
                                        <option value="Other">Other</option>
                                    </select>
                                </td>
                            </tr>
                            <tr>
                                <td align="right" valign="top" style="padding:2px 5px 0 0;">
                                    <label for="description">Comments</label>
                                </td>
                                <td>
                                    <textarea name="description">I would like you to contact me.</textarea>
                                </td>
                            </tr>
                            
                            <tr>
                                <td align="right" valign="top"></td>
                                <td align="left">
                                    <input type="submit" id="submit" name="submit" value="Submit">
                                </td>
                            </tr>
                        </table>
                    </form>
    
                    <!-- END WEB-2-LEAD FORM -->    
                </td>
                
                <td align="right" valign="top">
                    
                    <!-- if the company's web site has content in its right column, paste that content here --> 
                    <!-- make sure the right column content ends here -->
                    
                </td>
            </tr>
            <tr>
                <td colspan="3" align="center">
                
                    <!-- paste the company's footer starting here -->
        
                    <hr style="color:#999;height:1px;background-color:#999;border:0;margin:15px 0 10px 0;padding:0;" />
                    &copy; 2011 {!$Organization.Name}
                    
                    <!-- make sure the footer content ends here -->
                    
                </td>
            </tr>
        </tbody>
    </table>
</body>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-46606102-1', 'salesforce.com');
  ga('send', 'pageview');

</script>



</apex:page>